
{
	"manifest_version": 2,

	"name": "Jared Simulator",
	"description": "This extension will analyze a page using Jared",
	"version": "1.0",

	//"browser_action": {
		//"default_icon": "sweeterman.jpg",
		// "default_popup": "popup.html"
	//},

	"permissions": [
		"activeTab",
		"tabs",
		"http://*/*",
		"https://*/*"
   ],

	"content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
            	"mark.es6.js",
                "content.js"
            ],
            "css": [
            	"highlight.css"
            ],
            "run_at": "document_end"
        }
    ]
}