{
	"manifest_version": 2,

	"name": "Jared Simulator",
	"description": "This extension will analyze a page using Jared",
	"version": "1.0",

	//"browser_action": {
		//"default_icon": "sweeterman.jpg",
		// "default_popup": "popup.html"
	//},

	"permissions": [
		"activeTab"
   ],

	"content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "popup.js"
            ],
            "run_at": "document_end"
        }
    ]
}